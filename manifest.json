{
  "manifest_version": 2,

  "name": "Watch Later",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",
  "permissions": [
    "identity", "contextMenus", "http://*/*", "https://*/*" 
  ],
  "oauth2": {
    "client_id": "820911436798-dpbc64pf8qbnf1dvm1slu0ocma7g43nm.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/youtube"
    ]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com https://ajax.googleapis.com; object-src 'self'",
  "background": {
     "scripts": ["background.js", "restclient.js"]
  },
  "browser_action": {
    "default_icon": "icons/later.png",
    "default_popup": "popup.html",
    "default_title": "Watch Later"
  },
  "icons": { "16": "icons/later.png",
           "48": "icons/later48.png",
          "128": "icons/later.png" }
}
